// Run with jsdoc . -r -c conf.json -d docs
{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "source": {
        "include": ["Packages", "OHIFViewer", "LesionTracker"],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_",
        "exclude": ["Packages/cornerstone/", "node_modules", "docs", "etc"]
    },
    "plugins": ["plugins/markdown"],
    "templates": {
        "logoFile": "",
        "cleverLinks": false,
        "monospaceLinks": false,
        "dateFormat": "ddd MMM Do YYYY",
        "outputSourceFiles": false,
        "outputSourcePath": false,
        "systemName": "ScriptSender Meteor Packages",
        "footer": "ScriptSender",
        "copyright": "Copyright Â© 2018 ScriptSender",
        "navType": "vertical",
        "theme": "flatly",
        "linenums": true,
        "collapseSymbols": false,
        "inverseNav": true,
        "highlightTutorialCode": true,
        "protocol": "html://",
        "methodHeadingReturns": false
    },
    "markdown": {
        "parser": "gfm",
        "hardwrap": true
    }
}